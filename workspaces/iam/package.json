{
  "name": "iam",
  "version": "1.1.0",
  "description": "I'm(https://github.com/metaseed/iam writing your professional life!",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "preinstall": "node ./check-npm.js",
    "start": "ng serve --host=0.0.0.0",
    "hmr": "ng serve --configuration hmr --open --host=0.0.0.0",
    "build_raw": "ng build iam --configuration production --named-chunks --optimization=false",
    "build": "ng build iam --configuration production --named-chunks",
    "update:monaco": "ncp ../../node_modules/monaco-editor/min/ ./assets/monaco-editor/",
    "deploy": "ng build iam --configuration production --named-chunks --base-href /iam/ && angular-cli-ghpages --dir dist/iam && start http://metaseed.github.io/iam",
    "deploy_doc": "ng build iam --configuration production --output-path ../../docs  --base-href /iam/ && cd ../.. && git add . && git commit -am \"deploy\" && git push && start http://metaseed.github.io/iam",
    "test": "ng test",
    "lint": "ng lint",
    "wba": "npx webpack-bundle-analyzer dist/iam/stats.json",
    "ba": "ng build iam --configuration production --named-chunks --stats-json & npm run wba",
    "e2e": "ng e2e",
    "update": "ncu -u"
  },
  "engines": {
    "yarn": ">=1.3.2 <2.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/core": "^15.2.0",
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/cdk-experimental": "^15.2.0",
