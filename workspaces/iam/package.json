{
  "name": "iam",
  "version": "1.0.0",
  "description": "I'm(https://github.com/metaseed/iam writing your professional life!",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --source-map --host=0.0.0.0",
    "hmr": "ng serve --hmr --source-map --host=0.0.0.0",
    "build": "ng build --prod --named-chunks",
    "update:monaco": "ncp ../../node_modules/monaco-editor/min/ ./assets/monaco-editor/",
    "deploy": "ng build --prod --named-chunks --base-href /iam/ && angular-cli-ghpages --dir dist/iam && start http://metaseed.github.io/iam",
    "deploy_doc": "ng build --prod --output-path ../../docs  --base-href /iam/ && cd ../.. && git add . && git commit -am \"deploy\" && git push && start http://metaseed.github.io/iam",
    "test": "ng test",
    "lint": "ng lint",
    "ba": "ng build --prod --named-chunks --stats-json & npx webpack-bundle-analyzer dist/iam/stats.json",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": ">=8.9.1 <9.0.0",
    "yarn": ">=1.3.2 <2.0.0"
  },
