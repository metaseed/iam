    "@angular/router": "^15.2.0",
    "@angular/pwa": "^15.2.0",
    "@angular/service-worker": "^15.2.0",
    "base64-js": "^1.5.1",
    "codemirror": "^5.62.3",
    "fuse.js": "^6.6.2",
