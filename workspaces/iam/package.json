{
  "name": "iam",
  "version": "1.0.0",
  "description": "I'm(https://github.com/metaseed/iam writing your professional life!",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "hmr": "ng serve --port 4200 -hmr -e=hmr",
    "build": "ng build --prod -vendor-chunk=true",
    "update:monaco": "ncp ../../node_modules/monaco-editor/min/ ./assets/monaco-editor/",
    "deploy": "ng build --prod --base-href /iam/ && angular-cli-ghpages && start http://metaseed.github.io/iam",
    "deploy_doc": "ng build --prod --output-path ../../docs  --base-href /iam/ && cd ../.. && git add . && git commit -am \"deploy\" && git push && start http://metaseed.github.io/iam",
    "test": "ng test",
    "lint": "ng lint",
    "ba": "ng build --prod -vendor-chunk=true --stats-json & webpack-bundle-analyzer dist/stats.json",
    "e2e": "ng e2e"
  },
