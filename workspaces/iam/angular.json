{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "cache": {
      "enabled": true,
      "path": ".cache",
      "environment": "all"
    }
  },
  "newProjectRoot": "projects",
  "projects": {
    "iam": {
      "root": "",
      "sourceRoot": "src",
      "prefix": "ms",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/iam",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              "src/assets",
              "src/manifest.json",
              "src/.nojekyll"
            ],
            "styles": ["src/styles.scss", "src/styles/main.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/styles"]
            },
            "scripts": [],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "development": {
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "sourceMap": true,
              "optimization": false,
              "namedChunks": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            },
            "defaultConfiguration": "production"
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "defaultConfiguration": "",
          "options": {
            "browserTarget": "iam:build:development"
          },
          "configurations": {
            "production": {
              "browserTarget": "iam:build:production"
            },
            "hmr": {
              "hmr": true,
              "browserTarget": "iam:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "iam:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": ["src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["src/styles"]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min",
                "output": "/assets/monaco-editor"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              "src/assets",
              "src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    },
    "iam-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "iam:serve"
          }
        }
      }
    },
    "angular-hotkey": {
      "root": "projects/angular-hotkey",
      "sourceRoot": "projects/angular-hotkey/src",
      "projectType": "library",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/angular-hotkey/tsconfig.lib.json",
            "project": "projects/angular-hotkey/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/angular-hotkey/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-hotkey/src/test.ts",
            "tsConfig": "projects/angular-hotkey/tsconfig.spec.json",
            "karmaConfig": "projects/angular-hotkey/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular-hotkey/**/*.ts",
              "projects/angular-hotkey/**/*.html"
            ]
          }
        }
      }
    },
    "spinner": {
      "root": "projects/spinner",
      "sourceRoot": "projects/spinner/src",
      "projectType": "library",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/spinner/tsconfig.lib.json",
            "project": "projects/spinner/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/spinner/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/spinner/src/test.ts",
            "tsConfig": "projects/spinner/tsconfig.spec.json",
            "karmaConfig": "projects/spinner/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/spinner/**/*.ts",
              "projects/spinner/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "iam",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ms",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "ms"
    }
  }
}
